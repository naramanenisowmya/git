1)SELECT MAX(SAL)
FROM EMP
where JOB ='ANALYST'

2)SELECT DEPTNO,COUNT (*)
FROM EMP
GROUP BY DEPTNO

3)SELECT AVG(SAL) AVERAGE
FROM EMP
WHERE DEPTNO=10
GROUP BY DEPTNO;

4)SELECT MAX(COMM) AS 'HIGHEST COMM'
FROM EMP

5)SELECT 
AVG(SAL)
FROM EMP
WHERE JOB='MANAGER'

6)SELECT SUM(SAL) AS 'TOTAL SALARY'
FROM EMP
WHERE JOB='SALESMAN'

7)SELECT JOB,COUNT(*)
FROM EMP
GROUP BY JOB;

8)SELECT COUNT (DEPTNO)
FROM EMP;

9)SELECT MIN(SAL) LOWEST
FROM EMP
WHERE DEPTNO=10
GROUP BY ENAME;

self joins:

1)SELECT e.empno,e.hiredate,j.startdate,j.enddate
FROM EMP e,JOBHIST j
where e.hiredate between j.startdate and j.enddate;

2)SELECT E.ENAME,E.SAL, D.LOC
FROM EMP E,DEPT D

3)SELECT E.ENAME,D.LOC
FROM EMP E,DEPT D
WHERE LOC='CHICAGO'

4)SELECT E.ENAME,D.DNAME
FROM EMP E,DEPT D
WHERE E.DEPTNO=D.DEPTNO

5)select e.ename , c.ename
from emp e , emp c
where e.empno = c.mgr and e.HIREDATE < c.HIREDATE;

Using ANSI syntax:

1)SELECT 
e.ENAME,d.LOC
FROM EMP e
JOIN
DEPT d
ON d.DEPTNO = e.DEPTNO
order BY d.LOC;
